<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- 데이터베이스 컬럼명 A_COLUMN을 CamelCase형태의 자바 프로퍼트로 매핑 aColumn -->
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	<typeAliases>
		<!-- type별칭 VO이름을 간략히 사용 -->
		<typeAlias type="com.kkj.board.member.MemberVO" alias="MemberVO" />
		<typeAlias type="com.kkj.board.media.MediaVO" alias="MediaVO"/>
		<typeAlias type="com.kkj.board.comment.CommentVO" alias="CommentVO"/>
		<typeAlias type="com.kkj.board.board.BoardVO" alias="BoardVO"/>
		<typeAlias type="com.kkj.board.file.FileVO" alias="FileVO"/>
		<typeAlias type="com.kkj.board.workspace.WorkspaceVO" alias="WorkspaceVO"/>
		<typeAlias type="com.kkj.board.cmn.PageVO" alias="PageVO"/>
		<typeAlias type="com.kkj.board.workspace.WorkspaceMngVO" alias="WorkspaceMngVO"/>
		<typeAlias type="com.kkj.board.board.BoardRecommendVO" alias="BoardRecommendVO"/>
		<typeAlias type="com.kkj.board.comment.CommentRecommendVO" alias="CommentRecommendVO"/>
	</typeAliases>
</configuration>