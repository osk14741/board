<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kkj.board.media">
	<insert id="doInsert" parameterType="MediaVO">
		INSERT INTO media (
		    seq,
		    member_id,
		    img,
		    div
		) VALUES (
		    MEDIA_SEQ.nextVal,
		    #{memberId},
		    #{img},
		    #{div}
		)
	</insert>
	
	

</mapper>